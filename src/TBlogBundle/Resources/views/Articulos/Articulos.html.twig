<script type="text/javascript">
    $(document).ready(function() {
        $('select').material_select();
    });
    $("#buttonG").click(function () {
      var archivos = document.getElementById("file_url");
        var archivo = archivos.files[0];
        var descripcion = $("#des").val();
        var clasificacion = $("#idcla").val();

        var archivos = new FormData();

        archivos.append('archivo', archivo);
        archivos.append('descripcion', descripcion);
        archivos.append('clasificacion', clasificacion);


        $.ajax({
            url: "{{ path ('Importar') }}",
            type: 'POST',
            contentType: false,
            data: archivos,
            processData: false,
            cache: false,
        })
    });
</script>

